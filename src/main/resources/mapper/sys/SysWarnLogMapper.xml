<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lik.robotcms.mapper.SysWarnLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ifast.sys.domain.SysWarnLog">
        <id column="id" property="id" />
        <result column="user" property="user" />
        <result column="info" property="info" />
        <result column="start_time" property="startTime" />
        <result column="end_time" property="endTime" />
        <result column="place" property="place" />
        <result column="cancel_user" property="cancelUser" />
        <result column="code" property="code" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user, info, start_time AS startTime, end_time AS endTime, place, cancel_user AS cancelUser, code
    </sql>

</mapper>
