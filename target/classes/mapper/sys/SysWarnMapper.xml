<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lik.robotcms.mapper.SysWarnMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ifast.sys.domain.SysWarn">
        <id column="id" property="id" />
        <result column="warn_name" property="warnName" />
        <result column="status" property="status" />
        <result column="receiver" property="receiver" />
        <result column="content" property="content" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, warn_name AS warnName, status, receiver, content
    </sql>

</mapper>
